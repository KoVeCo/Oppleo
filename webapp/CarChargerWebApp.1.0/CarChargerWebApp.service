[Unit]
Description=uWSGI instance CarCharger Web Application
After=syslog.target network.target

[Service]
; Raspberry user for this service to run
User=pi
Group=www-data
WorkingDirectory=/home/pi/development/RestfulCharging/src/nl/carcharging/daemon
;Environment="PATH=/home/pi/RestfullCharging/venv/binâ€œ
ExecStart=/home/pi/.local/bin/uwsgi --ini /home/pi/development/RestfulCharging/webapp/CarChargerWebApp.ini
; uwsgi --socket 0.0.0.0:5000 --protocol=http -w WebAppTestDaemon:app -H /home/pi/development/RestfulCharging/venv 

[Install]
WantedBy=multi-user.target
